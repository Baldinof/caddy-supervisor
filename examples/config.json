{
  "logging": {
    "logs": {
      "default": {
        "exclude": [
          "http.log.access.log0"
        ]
      },
      "log0": {
        "writer": {
          "output": "stdout"
        },
        "include": [
          "http.log.access.log0"
        ]
      }
    }
  },
  "apps": {
    "supervisor": {
      "supervise": [
        {
          "command": ["php-fpm"],
          "dir":  ".",
          "env": {"ADDITIONAL_ENV": "1"},
          "redirect_stdout":  "stdout",
          "redirect_stderr": "stderr",
          "restart_policy":  "always",
          "termination_grace_period": "1s",
          "replicas":  "1"
        }
      ]
    }
  }
}